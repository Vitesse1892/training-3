{
  "name": "nationale-nederlanden-training-app",
  "version": "1.0.0",
  "description": "Complete training session management application with React frontend and Node.js backend",
  "scripts": {
    "start": "./start.sh",
    "start:dev": "./start.sh",
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "init-db": "cd backend && npm run init-db",
    "clean": "rm -rf backend/node_modules frontend/node_modules backend/training.db"
  },
  "keywords": [
    "training",
    "sessions",
    "react",
    "nodejs",
    "sqlite",
    "playwright"
  ],
  "author": "Champions Training Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/nationale-nederlanden-training-app.git"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/node": "^25.0.10"
  }
}
